class Dog{
  method init(name, height, weight){
    self.name = name;
    self.height = height;
    self.weight = weight;
  }

  method greet(){
    return "Hello, there! My name is " + self.name + " and I have " + self.height + "m and " + self.weight + "kgs.";
  }
}

let d = Dog("Thor", 1.5, 42);

print d.name;
print d.height;
print d.weight;

print d.greet();

d.name = "Sirius";
d.height = 1.23;
d.weight = 64;

print d.greet();