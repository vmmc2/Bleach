class Cat{
  method init(name, age, breed){
    self.name = name;
    self.age = age;
    self.breed = breed;
  }

  method str(){
    return "Hi! I am a cat. My name is " + self.name + ". I am " + self.age + " years old. My breed is " + self.breed + ".";
  }
}

let c = Cat("Yoruichi", 12, "Scottish Fold");

print c;