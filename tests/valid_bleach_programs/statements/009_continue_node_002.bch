// This test is responsible for checking whether the 'Continue' node is correctly functioning. 
// Here, we check a scenario whether a nested 'for' has its behavior changed by the use of a "continue"
// statement.

for(let i = 1; i <= 4; i = i + 1){
  for(let j = 1; j <= 4; j = j + 1){
    if(i % 2 == 1){
      continue;
    }
    print "i: " + i + " --- j: " + j;
  }
}